{
  "name": "magic-life-counters",
  "version": "1.2.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "build": "ng build --output-path cordova/www/",
    "production-build": "ng build --output-path cordova/www/ --prod=\"true\"",
    "android-init": "cd cordova && cordova prepare",
    "android-start": "npm run build && npm run android-run",
    "android-run": "cd cordova && cordova run android",
    "android-apk": "npm run production-build && cd cordova && cordova build --prod --release android",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "core-js": "^2.6.11",
    "hammerjs": "^2.0.8",
    "html2canvas": "^1.0.0-rc.5",
    "jquery": "^3.4.1",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "^8.3.22",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/node": "^12.12.24",
    "codelyzer": "^5.2.1",
    "ts-node": "~8.4.0",
    "tslint": "6.0.0-beta0",
    "typescript": "~3.5.0"
  }
}
